root=true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 4
indent_style = space

[*.{diff, md}]
trim_trailing_whitespace = false

[*.java]
ij_java_imports_layout = $*, |, java.**, javax.**, |, *, |, io.task406.**
ij_java_use_single_class_imports = true
